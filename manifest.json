{
	"manifest_version": 2,
	"name": "Chrome Context2MUI",
	"description": "Extension designed for open MUI with the selected ID.",
	"version": "1.00",
	"icons": { "48": "icon48.png"
	},
	
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Chrome Context2MUI",
		"default_popup": "context2mui.html"
	},
		
	"permissions": [
		"contextMenus"
	],
	
	"web_accessible_resources": [
		"bg.jpg",
		"icon48.png"
	]
}